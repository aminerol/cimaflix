{
  "name": "cimaflix",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "npm run build",
    "postbuild": "cpx '.htaccess' dist/public/ && cpx 'ie-polyfills.min.js' dist/public/assets/js && cpx 'robots.txt' dist/public/ ",
    "build": "ng build --prod --aot --delete-output-path=false --vendor-chunk=false --output-hashing=none && tsc --project server/tsconfig.app.json",
    "build-dev": "ng build --delete-output-path=false && tsc --project server/tsconfig.app.json",
    "start-dev": "pm2 start dist/server/www.js --no-daemon",
    "start": "pm2 start dist/server/www.js",
    "build-start": "npm run build && npm start",
    "build-start-dev": "npm run build-dev && npm start-dev",
    "dev": "concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run api\" ",
    "api": "cd server && nodemon --ext ts --ignore *.spec.ts --exec ts-node www",
    "build-ie-polyfills": "yarn webpack-cli client/ie-polyfills.js -o client/assets/js/ie-polyfills.min.js --mode production",
    "test": "concurrently \"ng test --progress false\" \"npm run test-server\" ",
    "test-ng": "ng test",
    "test-server": "cd server && nodemon --ext ts --exec jasmine-ts **/*[!.api].spec.ts",
    "test-api": "cd server && nodemon --ext ts --exec jasmine-ts **/*.api.spec.ts",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "1.7.4",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/compiler-cli": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/router": "4.4.4",
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.34",
    "@types/mongoose": "^4.7.1",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.3",
    "@types/multer-s3": "^2.7.2",
    "@types/node": "~6.0.60",
    "angulartics2": "^4.6.3",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.3.3",
    "express": "^4.17.3",
    "express-sitemap": "^1.8.0",
    "memory-cache": "^0.2.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "ng2-file-upload": "^1.2.0",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-pagination": "^3.1.1",
    "node-cache": "^4.2.0",
    "pm2": "^2.4.0",
    "require-all": "^2.1.0",
    "rxjs": "^5.1.0",
    "script-loader": "^0.7.2",
    "sitemap": "^1.13.0",
    "typescript": "2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.38",
    "@types/nock": "^8.2.1",
    "@types/supertest": "^2.0.0",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.1.0",
    "cpx": "1.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jasmine": "2.5.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jasmine-ts": "0.0.3",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nock": "^9.0.9",
    "node-mocks-http": "^1.6.1",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "supertest": "^3.0.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "uglify-js": "^3.4.4",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.14"
  }
}
